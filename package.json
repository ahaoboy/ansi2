{
  "name": "ansi2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "update:snapshot": "INSTA_UPDATE=always cargo test",
    "pre-check": "cargo fmt && cargo clippy --fix --allow-dirty --allow-staged && cargo test && npm run format && npm run lint:fix",
    "format": "biome format --write  ./",
    "lint:fix": "biome lint ./ --write --unsafe"
  },
  "description": "ansi2",
  "main": "dist/index.js",
  "bin": "./bin/cli.js",
  "types": "./dist/index.d.ts",
  "author": "ahaoboy",
  "license": "MIT",
  "homepage": "https://github.com/ahaoboy/ansi2",
  "files": ["dist", "bin"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahaoboy/ansi2.git"
  },
  "devDependencies": {
    "lefthook": "1.7.18",
    "@biomejs/biome": "1.9.3",
    "@types/node": "22.7.7",
    "esbuild": "0.25.4",
    "typescript": "^5.6.3",
    "wasm-pack-inline": "0.1.4",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.13.0",
    "npm-run-all": "^4.1.5"
  }
}
